{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue",
		"server/writeToCsv.js",
		"server/writeToJson.js",
		"server/writeToCsv.js",
		"server/writeToJson.js"
	],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"types": ["node"],
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
